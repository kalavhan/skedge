<%= form_for(:session, :html => { :class => "form-login" }) do |f| %>
  <%= f.text_field :email, placeholder: "Email"%>
  <%= f.password_field :password, placeholder: "Password" %>
  <div class="field check-box">
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me %>
  </div>
  <%= f.submit "Login"%>
  <%= %>
<% end %>